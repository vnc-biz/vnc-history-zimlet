<zimlet name="@ZIMLET_NAME@" version="@ZIMLET_VERSION@" label="${msg.zimletLabel}" description="${msg.zimletDescription} (@ZIMLET_VERSION@)">
	<include>biz_vnc_lightweight_history.js</include>
	<includeCSS>lightweightmailhistory.css</includeCSS>
	<handlerObject>biz_vnc_lightweight_history_HandlerObject</handlerObject>
